    "debug": "*",
    "memorizer": "1",
    "semver": "4",
    "useragent": "2"
  },
  "devDependencies": {
    "autoprefixer-core": "4",
    "es6-arrow-function": "0",
    "es6-class": "0",
    "es6-computed-properties": "0",
    "es6-default-params": "0",
